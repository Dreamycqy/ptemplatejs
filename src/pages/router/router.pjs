<script>
	$.router({
		"/home": function(e){
			this._set("my-page", {
				type: "home"
			});
		},
		"/view": function(e, args){
			this._set("my-page", {
				type: "view",
				index: args && args.index || 0
			});
		},
		"/github": function(e){
			location.href = "https://github.com/ereddate/ptemplatejs";
		}
	});
</script>